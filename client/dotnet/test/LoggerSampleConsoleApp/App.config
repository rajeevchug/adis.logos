<?xml version="1.0"?>
<configuration>
	<system.serviceModel>
		<client>
			<endpoint address="http://devsrv02/AdisLog/logger.svc" binding="wsHttpBinding"
								contract="Adis.Log.Contract.ILoggingContract" bindingConfiguration="wsHttpBinding_Insecure"
								name="Logger"/>
		</client>
		<bindings>
			<wsHttpBinding>
				<binding name="wsHttpBinding_Insecure">
					<security mode="None"/>
				</binding>
			</wsHttpBinding>
		</bindings>
	</system.serviceModel>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/></startup></configuration>
