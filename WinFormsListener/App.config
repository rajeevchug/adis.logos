<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<system.serviceModel>
		<bindings>
			<netTcpBinding>
				<binding name="NonsecureTcp" >
					<security mode="None"/>
				</binding>
			</netTcpBinding>
		</bindings>
		<client>
			<endpoint address="net.tcp://localhost:8732/Design_Time_Addresses/Logos/ListenerService"
								binding="netTcpBinding"
								bindingConfiguration="NonsecureTcp"
								contract="Logos.Contract.IListenerContract"
								name="NetTcpBinding" />
		</client>
	</system.serviceModel>
</configuration>